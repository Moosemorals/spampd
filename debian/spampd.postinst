#!/bin/bash -e

handle_init_errs ()
{
        RET=$?
        if [ "$RET" = "6" ]; then
                return 0
        else
                exit $RET
        fi
        set -e
}

case "$1" in 
	install|configure)
		adduser --system --group \
		        --no-create-home --home /nonexistent \
			--quiet spampd 
		;;
	*)
		;;
esac

